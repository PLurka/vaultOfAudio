<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity ListSong.
    -->
    <changeSet id="20200218212701-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="list_id_id"
                                 baseTableName="list_song"
                                 constraintName="fk_list_song_list_id_id"
                                 referencedColumnNames="id"
                                 referencedTableName="playlist"/>

        <addForeignKeyConstraint baseColumnNames="song_id_id"
                                 baseTableName="list_song"
                                 constraintName="fk_list_song_song_id_id"
                                 referencedColumnNames="id"
                                 referencedTableName="song"/>

    </changeSet>
</databaseChangeLog>
